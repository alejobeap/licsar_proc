  if [ -z "$5" ]; then local IW1=1; else local IW1=$5; fi 
  if [ -z "$6" ]; then local IW3=3; else local IW3=$6; fi 
  # Identify which bursts in master are inside the polygon, before starting to merge
}
check_missing_bursts $@
